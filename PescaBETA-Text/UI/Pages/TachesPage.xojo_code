#tag WindowCode
	#tag Event
		Sub Close()
		  //image du menu
		  Self.MenuContainerData.ProjetsImg.Picture = Projets50x50
		  Self.TachesIdProjet = ""
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown()
		  //Image du menu
		  TachesPage.MenuContainerData.ProjetsImg.Picture = ProjetsOra50x50
		  
		  //Déterminer la taille des colonnes dans le container
		  Self.SBCTaches.TasksListBox.ListIndex = 0
		  
		  
		  ////Afficher Container
		  Self.DGTContainerModify.Visible = False
		  Self.DGTContainer.Visible = True
		  
		  //Alimenter les tâches
		  FeedListeTache(Self.SBCTaches.TasksListBox, MainPage.SideBarContainerMain.ProjetsListBox.RowTag(MainPage.SideBarContainerMain.ProjetsListBox.ListIndex))
		  
		  //Alimenter les listes
		  Self.FeedEmp(TachesPage.DGTContainer.RespPopupMenu)
		  Self.FeedStatut(TachesPage.DGTContainer.StatutPopupMenu)
		  Self.FeedTypeTache(TachesPage.DGTContainer.TypePopupMenu)
		  
		  
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub EffacerTache()
		  If Self.SBCTaches.TasksListBox.ListIndex >=0 Then  'Une tâche est sélectionnée
		    Dim strSQL As String
		    Dim intIndex As Integer
		    
		    //Effacer la tâche
		    intIndex = Self.SBCTaches.TasksListBox.ListIndex
		    strSQL = "DELETE FROM taches WHERE id_tache = " + Self.SBCTaches.TasksListBox.RowTag(intIndex) + ""
		    Session.db.SQLExecute(strSQL)
		    
		    If Session.db.Error Then
		      ActivateWebDialog("Erreur de la base de données:  " + Session.db.ErrorMessage, "P")
		    End If
		    
		    //Mettre à jour de la liste des tâches
		    Self.UpdateTaches(Self.SBCTaches.TasksListBox, Self.TachesIdProjet)
		    
		    //S'il reste des tâches pour ce projet...
		    If Self.SBCTaches.TasksListBox.RowCount >1 Then
		       intIndex = 0
		      // Ajouter les valeur aux champs
		      Dim strSQL2 As String
		      strSQL = "SELECT * FROM taches WHERE id_tache = '" + Self.SBCTaches.TasksListBox.RowTag(IntIndex) + "'"
		      
		      Dim RC As RecordSet
		      RC = Session.db.SQLSelect(strSQL2)
		      
		      If Session.db.Error Then
		        ActivateWebDialog("Erreur de la base de données:  " + Session.db.ErrorMessage, "P")
		      End If
		      
		      If RC <> Nil Then
		        Self.DGTContainer.ProjetActifLabel.Text = Self.GetProjet(RC.Field("lien_projet").StringValue)
		        Self.DGTContainer.NomField.Text = RC.Field("nom_tache").StringValue
		        Self.DGTContainer.DescTextArea.Text = RC.Field("desc_tache").StringValue
		        Self.DGTContainer.RespPopupMenu.Text = Self.GetREspName(RC.Field("responsable").StringValue)
		        Self.DGTContainer.StatutPopupMenu.Text = Self.GetStatut(RC.Field("statut_tache").StringValue)
		        Self.DGTContainer.TypePopupMenu.Text = Self.GetType(RC.Field("type_tache").StringValue)
		      End If
		      
		      RC.Close
		      RC = Nil
		      
		      strSQL = ""
		    Else
		      Self.DGTContainer.ClearContainerTextFields
		      Self.DGTContainer.ClearContainerPopUpMenus
		    End if
		  End If
		  
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub FeedEmp(PopUpMenu As WebPopupMenu)
		  // Alimenter les roles
		  
		  PopUpMenu.DeleteAllRows
		  Dim RC As RecordSet
		  Dim strSQL As String
		  
		  strSQL = "SELECT * FROM users ORDER BY user_name"
		  RC = Session.db.SQLSelect(strSQL)
		  
		  If RC <> Nil Then
		    RC.MoveFirst
		    While Not RC.EOF
		      PopUpMenu.AddRow(RC.Field("user_name").StringValue)
		      PopUpMenu.RowTag(PopUpMenu.ListCount-1) = RC.Field("user_id").IntegerValue
		      RC.MoveNext
		    Wend
		  End If
		  
		  RC.Close
		  RC = Nil
		  
		  strSQL = ""
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub FeedSites(PopUpMenu As WebPopupMenu)
		  // Alimenter les roles
		  
		  PopUpMenu.DeleteAllRows
		  Dim RC As RecordSet
		  Dim strSQL As String
		  
		  If Self.SBCTaches.TasksListBox.RowCount >= 1 And SBCTaches.TasksListBox.ListIndex <> -1 Then
		    strSQL = "SELECT * FROM sites WHERE id_tache = '" + Self.SBCTaches.TasksListBox.RowTag(SBCTaches.TasksListBox.ListIndex) + "'"
		    RC = Session.db.SQLSelect(strSQL)
		  End If
		  
		  If RC <> Nil Then
		    RC.MoveFirst
		    While Not RC.EOF
		      PopUpMenu.AddRow(RC.Field("nom_site").StringValue)
		      PopUpMenu.RowTag(PopUpMenu.ListCount-1) = RC.Field("id_site").IntegerValue
		      RC.MoveNext
		    Wend
		    RC.Close
		    RC = Nil
		    strSQL = ""
		  End If
		  
		  
		  
		End Sub
	#tag EndMethod

