		Function GetProjet(strID As String) As String
		  Dim strSQL As  String
		  Dim RC As RecordSet
		  Dim strToFind As String
		  
		  strSQL = "SELECT nom_projet FROM projets WHERE id_projet = '" + strID+ "'"
		  RC = Session.db.SQLSelect(strSQL)
		  If RC <> Nil Then
		    strToFind = RC.Field("nom_projet").StringValue
		  End If
		  
		  RC.Close
		  RC = Nil
		  
		  strSQL = ""
		  
		  Return strToFind
		End Function
