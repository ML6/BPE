#tag WindowCode
	#tag Event
		Sub Shown()
		  If Session.db.Connect Then
		    Session.Connected = True
		    DbConnWheel.Visible = False
		    ConnectStatusLabel.Text = Session.SuccessConnection
		    LoginButton.Enabled = True
		    LoginButton.Style = ButtonColorStyle
		  Else
		    Session.Connected = False
		    ConnectStatusLabel.Text = Session.ErrorConnection
		  End If
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function LoginValidation() As Boolean
		  //D√©celer l'absence des param√®tres d'ouverture de session
		  If Not (UserNameField.Text = "" Or PWField.Text = "") Then
		    //V√©rifier les param√®tres
		    Dim strSQL As String
		    strSQL = "SELECT * FROM users WHERE user_login =  '" + UserNameField.Text + "'"
		    
		    Dim RS As RecordSet
		    RS = Session.db.SQLSelect(strSQL)
		    
		    If RS <> Nil Then
		      If Session.db.Error Then
		        ActivateWebDialog("V√©rifiez votre nom d'utilisateur et votre mot de passe recommencez.", "P")
		        Return False
		      End If
		      If UserNamefield.Text = RS.field("user_login").StringValue And pwfield.Text = RS.Field("user_password").StringValue Then
		        User.UserRole = RS.Field("user_roles")
		        User.UserFullName = RS.Field("user_name")
		        Return True
		      End If
		    Else
		      Return False
		    End If
		    
		  Else
		    UserNameField.Style = TextFieldErrorStyle
		    PWField.Style = TextFieldErrorStyle
		    ActivateWebDialog("V√©rifiez votre nom d'utilisateur et votre mot de passe recommencez.", "P")
		    Return False
		  End If
		  
		  
		  
		  
		  
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub UpdateSessionLoginAndPW()
		  
		  If LoginValidation Then
		    //Mettre √† jour  le cookie
		    If MemoCheckbox.Value Then
		      Session.Cookies.Set("UserName", UserNameField.Text)
		    Else
		      Session.Cookies.Remove("UserName")
		    End If
		    
		    Self.Close
		    Session.UserName = UsernameField.Text
		    Session.Connected = True
		    Accueil.Show
		    
		  Else
		    ActivateWebDialog("Vos param√®tres de connexion sont erron√©s. Entrez-les √† nouveau.", "P")
		  End If
		End Sub
	#tag EndMethod


	#tag Constant, Name = KeyPressed, Type = , Dynamic = False, Default = \"", Scope = Public
	#tag EndConstant


#tag EndWindowCode

#tag Events PWField
	#tag Event , Description = 456E746572204B6579
		Sub KeyPressed(Details As REALbasic.KeyEvent)
		  Select Case Details.KeyCode
		  Case Details.KeyEnter
		    Call LoginValidation
		    UpdateSessionLoginAndPW
		  End Select
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events LoginButton
	#tag Event
		Sub Action()
		  Call LoginValidation
		  
		  UpdateSessionLoginAndPW
		End Sub
	#tag EndEvent
#tag EndEvents
